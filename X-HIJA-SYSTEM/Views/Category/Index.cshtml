@model List<Category>
<h1>Welcome to category page !</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Category Name</th>
        <th>Display Order </th>
    </tr>
    </thead>
    <tbody>
        @foreach(var list in Model){
        <tr>
                <td>@list.CatName</td>
                <td>@list.DisplayOrder </td>
        </tr>
        }
    </tbody>
</table>